package example:hello-world-rust-wasm-component@1.0.0;

interface hello-world-rust-wasm-component-interface {
  record report {
    bouncy-castles: u32,
    fun-percent: float64,
    cat-count: u32,
    unicorn-names: list<string>,
  }

  record point {
    x: float64,
    y: float64,
  }

  record circle {
    center: point,
    radius: float64,
  }
}

world hello-world-rust-wasm-component {
  import print: func(msg: string);

  export run: func();

  export greet: func(name: string) -> string;
  export greet-many: func(names: list<string>) -> string;

  use hello-world-rust-wasm-component-interface.{report};
  export get-report: func() -> report;

  use hello-world-rust-wasm-component-interface.{circle};
  export compute-area: func(circle: circle) -> float64;
}
